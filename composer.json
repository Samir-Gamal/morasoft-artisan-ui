{
  "name": "morasoft/artisan-ui",
  "description": "A Laravel package providing a GUI to run artisan commands.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Samir Gamal",
      "email": "your-email@example.com",
      "homepage": "https://github.com/Samir-Gamal",
      "role": "Developer"
    }
  ],
  "autoload": {
